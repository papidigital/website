{
  "name": "papidigital2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true scss/ -o assets/css",
    "build-task:bootstrap-scss-compile": "node-sass-chokidar --source-map true node_modules/bootstrap/scss/ -o assets/css",
    "build-task:autoprefixer": "postcss node_modules/bootstrap/css/*.css assets/css/*.css --use autoprefixer -d css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'scss/**/*.scss' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.7.3",
    "chokidar-cli": "^2.1.0",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  },
  "devDependencies": {
    "bootstrap": "4.4.1"
  }
}
